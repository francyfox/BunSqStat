{
	"name": "server",
	"version": "1.0.50",
	"exports": {
		".": "./src/routes.ts",
		"./schema": "./src/export.ts"
	},
	"scripts": {
		"dev": "bun --env-file=../../.env run --watch src/index.ts --dev",
		"test": "bun test",
		"build": "bun run build.ts",
		"build:binary": "bun build src/index.ts --compile --target=bun-linux-x64-baseline --outfile=bunsqstat-backend-binary",
		"preview": "bun --env-file=../../.env ./dist/index.js",
		"test:logs": "bun run src/__tests__/log-generator/log-simulator.ts",
		"test:logs:quick": "bun run src/__tests__/log-generator/log-simulator.ts --duration=30"
	},
	"dependencies": {
		"@elysiajs/cors": "^1.4.0",
		"@elysiajs/swagger": "^1.3.1",
		"@sentry/bun": "^10.24.0",
		"ajv": "^8.17.1",
		"chokidar": "^4.0.3",
		"elysia": "^1.4.15",
		"elysia-rate-limit": "^4.4.2",
		"logixlysia": "^5.3.0",
		"mathjs": "^15.1.0",
		"mime": "^4.1.0"
	},
	"devDependencies": {
		"@sinclair/typebox": "^0.34.41",
		"@types/node": "^24.10.0",
		"bun-types": "^1.3.2",
		"nanoid": "^5.1.6"
	},
	"module": "src/index.js"
}
