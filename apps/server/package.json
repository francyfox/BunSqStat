{
	"name": "server",
	"version": "1.0.50",
	"exports": {
		".": "./src/routes.ts",
		"./schema": "./src/export.ts"
	},
	"scripts": {
		"dev": "bun --env-file=../../.env run --watch src/index.ts",
		"test": "bun test",
		"build": "bun run build.ts",
		"preview": "bun --env-file=../../.env ./dist/index.js",
		"test:logs": "bun run src/__tests__/log-generator/log-simulator.ts",
		"test:logs:quick": "bun run src/__tests__/log-generator/log-simulator.ts --duration=30"
	},
	"dependencies": {
		"@elysiajs/cors": "^1.3.3",
		"@elysiajs/swagger": "^1.3.1",
		"ajv": "^8.17.1",
		"chokidar": "^4.0.3",
		"elysia": "latest",
		"elysia-rate-limit": "^4.4.0"
	},
	"devDependencies": {
		"bun-types": "latest"
	},
	"module": "src/index.js"
}
